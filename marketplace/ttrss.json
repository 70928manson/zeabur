{
  "$schema": "./schema.json",
  "code": "ttrss",
  "name": "Tiny Tiny RSS",
  "description": "Tiny Tiny RSS is a free and open source web-based news feed (RSS/Atom) reader and aggregator",
  "icon": "https://tt-rss.org/favicon.ico",
  "image": "wangqiru/ttrss",
  "command": null,
  "args": null,
  "port": 80,
  "network": "HTTP",
  "variables": [
    {
      "name": "DB_HOST",
      "value": "${POSTGRES_HOST}"
    },
    {
      "name": "DB_PORT",
      "value": "${POSTGRES_PORT}"
    },
    {
      "name": "DB_NAME",
      "value": "ttrss"
    },
    {
      "name": "DB_USER",
      "value": "${POSTGRES_USERNAME}"
    },
    {
      "name": "DB_PASS",
      "value": "${POSTGRES_PASSWORD}"
    },
    {
      "name": "PORT",
      "value": "80"
    },
    {
      "name": "SELF_URL_PATH",
      "value": "https://your-domain.tld"
    }
  ]
}